@model IEnumerable<PF_Pach_OS.Models.Insumo>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomInsumo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CantInsumo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Medida)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomInsumo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CantInsumo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Medida)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdInsumo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdInsumo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdInsumo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>

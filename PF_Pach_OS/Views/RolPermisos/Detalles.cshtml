@model PF_Pach_OS.Models.Asignar_Permisos

@{
    ViewData["Title"] = "Detalles";
    Layout = null;
}
<h5 id="titulo-modal">Detalles Rol</h5>
<div class="row">
    <div class="col-md-4">
      
            
            <div class="form-group">
            <label class="control-label">@ViewBag.NomRol </label>
               
                
            </div>
            <div class="form-group row">
                <label class="control-label">Permisos</label>
                @if (ViewBag.Permisos != null)
                {
                    @foreach (var valor in (List<dynamic>)ViewBag.Permisos)
                    {
                        <label class="col-10">@valor.nom_permiso</label>
                    }

                }
                else
                {
                    <p>La lista de permisos es nula.</p>
                }
            </div>
        
    </div>
</div>


<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/select2@4.0.13/dist/js/select2.full.min.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
